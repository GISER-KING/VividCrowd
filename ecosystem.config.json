{
  "apps": [
    {
      "name": "vividcrowd-frontend",
      "script": "npm",
      "args": "run dev",
      "cwd": "./frontend",
      "interpreter": "none",
      "env": {
        "NODE_ENV": "production"
      },
      "watch": false,
      "max_memory_restart": "500M",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "error_file": "./logs/frontend-error.log",
      "out_file": "./logs/frontend-out.log",
      "merge_logs": true
    }
  ]
}
